  "name": "Loot Counter",
  "description": "Counts and displays collected loot chests in Strikes. Requires Spoof World Names to be enabled in PEB.",
  "authors": [
    "Vladomeme"
  ],
  "contact": {
  },

  "license": "MIT",
  "icon": "assets/lootcounter/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.lootcounter.main.CounterInit"
    ]
  },
  "mixins": [
    "lootcounter.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17"
  }
}
